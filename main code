from random import randint 
a = []
b = []
for i in range(12):
   ran_num = randint(0,100)
   a.append(ran_num)
   ran_num2 = randint(0,100)
   b.append(ran_num2) 
print(a)
print(b)

def intersection(a, b):
    mix_list = [value for value in a if value in b]
    return mix_list
print(intersection(a, b))    
